<h1>Users#show</h1>
<p>Find me in app/views/users/show.html.erb</p>

<div>
  <h3><%= @user.name %></h3>
  <%= image_tag @user.get_profile_image(100,100) %>
</div>



<% books.each do |book| %>
<div>
  <p>投稿ユーザー画像：<%= image_tag book.user.get_profile_image(100,100) %></p>
  <p>Title<%= book.title %></p>
  <p>Option<%= book.body %></p>
</div>
<% end %>
